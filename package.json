{
  "name": "nqm-app-minimal",
  "private": true,
  "version": "1.0.0",
  "description": "Minimal application for use with the updated application account system.",
  "main": "databot.js",
  "scripts": {
    "deploy": "npm run databotify && DEBUG=nqm:deploy node ./deploy-application.js",
    "databotify": "npm run build && zip -r databot.zip databot.js server dist package.json",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=development DEBUG=nqm-app:* node ./server/index.js --config ./settings.js",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production DEBUG=nqm-app:* node ./server/index.js --config ./settings.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nqminds/nqm-app-minimal.git"
  },
  "keywords": [],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/nqminds/nqm-app-minimal/issues"
  },
  "homepage": "https://github.com/nqminds/nqm-app-minimal#readme",
  "author": "nqminds",
  "contributors": [
    {
      "name": "Toby Ealden",
      "email": "toby.ealden@gmail.com"
    },
    {
      "name": "Ivan Glautier",
      "email": "ivan@nqminds.com"
    },
    {
      "name": "Przemyslaw Jamrozowicz",
      "email": "pjamrozowicz@gmail.com"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "circular-dependency-plugin": "^5.2.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "react-app-polyfill": "^1.0.4",
    "style-loader": "^1.0.0",
    "svg-url-loader": "^3.0.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@nqminds/nqm-api-tdx": "^0.9.1",
    "@nqminds/nqm-tdx-client": "^0.9.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "nqm-mantra-core": "^1.8.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
